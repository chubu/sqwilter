{
  "manifest_version": 2,
  "name": "Sqwilter",
  "description": "$(pkg .description)",
  "version": "$(pkg .version)",
  "content_scripts": [
    {
      "matches": [
        "https://app.sqwiggle.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "https://app.sqwiggle.com/*"
  ],
  "web_accessible_resources": [
    "exec.js"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
